<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6//EN" "http://google-web-toolkit.googlecode.com/svn/releases/1.6/distro-source/core/src/gwt-module.dtd">

<!-- The module will be renamed from 'cktest' because 'de.chkal.gwt.demo.App' is ugly -->
<module rename-to="cktest">

  <!-- Errai extensions -->
  <!-- 
  <inherits name="org.jboss.errai.common.ErraiCommon" />
  <inherits name="org.jboss.errai.bus.ErraiBus" />
  <inherits name="org.jboss.errai.ioc.Container" />
  <inherits name="org.jboss.errai.enterprise.CDI" />
  -->
 
  <!-- Always required? May also be pulled in by the Errai stuff -->
  <inherits name="com.google.gwt.user.User"/>
  
  <!-- Alternative for using the Errai annotation -->
  <entry-point class="de.chkal.gwt.demo.client.MyModule"/>

  <!-- Only generate Safari (Webkit/Chrome) code -->
  <set-property name="user.agent" value="safari" />

</module>
